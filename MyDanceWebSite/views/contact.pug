extends base.pug
block script
    script
        include ../static/index.js
block style
        style
            include ../static/style.css
            include ../static/styleContact.css
block content
    div.container
        h1 Contact Us
        form(action="/contact", method="post" class="myform")
            input(type="text",name="Name",class="inputclass",placeholder="Enter Your Name")
            input(type="phone",name="Phone",class="inputclass",placeholder="Enter Your Phone")
            input(type="email",name="Email",class="inputclass",placeholder="Enter Your Email")
            input(type="text",name="Gender",class="inputclass",placeholder="Enter Your Gender")
            input(type="text",name="Address",class="inputclass",placeholder="Enter Your Address")
            input(type="text",name="Dancestyle",class="inputclass",placeholder="Enter Your Dance style")
            button(class="btn")
                |Submit
    